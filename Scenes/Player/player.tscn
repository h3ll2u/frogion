[sub_resource type="GDScript" id="GDScript_oebue"]
script/source = "extends Node2D


@onready var fire_flies = $FireFlies
@onready var animation_player = $\"../AnimationPlayer\"


var day_state 


func _ready():
	Signals.connect(\"day_time\", Callable(self, \"_on_change_time\"))
	

func _process(delta):
	if Input.is_action_just_pressed(\"ui_down\"):
		print(day_state)
		print(fire_flies.emitting)


func _on_change_time(state, day_count):
	day_state = state
	if state == 1:
		fire_flies.emitting = true
	elif state == 2:
		fire_flies.emitting = false
	elif state == 3:
		fire_flies.emitting = true
	elif state == 4:
		fire_flies.emitting = true
"

